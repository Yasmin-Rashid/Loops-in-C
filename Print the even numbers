//Take n numbers as input, then print only the even numbers.

#include<stdio.h>
int main()
{
    int i;

    int numbers[10];

    printf("Enter the 10 numbers : ");
    for(i=0; i<=9; i++)
    {
        scanf("%d", &numbers[i]);
    }

    {
        printf("Even numbers are : \n");
        for(i=0; i<10; i++)
        {
            if(numbers[i] % 2 ==0)
            {
            printf("%d\n", numbers[i]);
            }
        }
    }
}
