//Take 10 integers as input and print them in reverse order.

#include<stdio.h>
int main()
{
    int i;
    int integers[10];

    for(i=0; i<10; i++)
    {
        printf("Enter the integers : ");
        scanf("%d", &integers[i]);
    }

    printf("The integers in reverse order : \n");
    for(i=9; i>=0; i--)
    {
        printf("%d\n", integers[i]);
    }
}
